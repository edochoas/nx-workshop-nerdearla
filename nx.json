{
  "npmScope": "starwars",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "admin": {
      "tags": []
    },
    "admin-e2e": {
      "tags": [],
      "implicitDependencies": ["admin"]
    },
    "api": {
      "tags": []
    },
    "data": {
      "tags": []
    },
    "ui-kit": {
      "tags": []
    },
    "wookieepedia": {
      "tags": []
    },
    "wookieepedia-e2e": {
      "tags": [],
      "implicitDependencies": ["wookieepedia"]
    }
  }
}
